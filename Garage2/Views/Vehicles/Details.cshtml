@model Garage2.Models.Vehicle

@{
    ViewData["Title"] = "Details";
}

    <head>
        <link rel="stylesheet" type="text/css" href="~/css/vehicles.css" />
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    </head>

<h1>Vehicle Details</h1>

<div>
    <hr />
    <div class="card" style="padding: 10px;">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.RegNr)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.RegNr)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Color)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Color)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Model)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Model)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Manufacturer)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Manufacturer)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NOWheels)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NOWheels)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Type)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Type)
            </dd>
            <dt class="col-sm-2">
                Time of parking:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.BeginParking)
            </dd>
        </dl>
    </div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" autofocus="autofocus" tabindex="1">
        Edit
        <i class="fa fa-pencil-square-o text-primary"></i>
    </a>|
    <a asp-action="Index" tabindex="2">Back to List</a>
</div>
